From 3caeba07981de09c440284caa5d3d31d686377d6 Mon Sep 17 00:00:00 2001
From: Sean Busbey <busbey@apache.org>
Date: Fri, 30 Jun 2017 09:15:44 -0500
Subject: [PATCH] HBASE-15391 ADDENDUM Avoid too large "deleted from META" info
 log.

---
 .../src/main/java/org/apache/hadoop/hbase/MetaTableAccessor.java      | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/hbase-client/src/main/java/org/apache/hadoop/hbase/MetaTableAccessor.java b/hbase-client/src/main/java/org/apache/hadoop/hbase/MetaTableAccessor.java
index 32f66b3..de79a67 100644
--- a/hbase-client/src/main/java/org/apache/hadoop/hbase/MetaTableAccessor.java
+++ b/hbase-client/src/main/java/org/apache/hadoop/hbase/MetaTableAccessor.java
@@ -1977,9 +1977,9 @@ public class MetaTableAccessor {
       deletes.add(e);
     }
     deleteFromMetaTable(connection, deletes);
-    LOG.info("Deleted " + regionInfos.size() + " regions from META");
+    LOG.info("Deleted " + regionsInfo.size() + " regions from META");
     if (LOG.isDebugEnabled()) {
-      LOG.debug("Deleted regions: " + regionInfos);
+      LOG.debug("Deleted regions: " + regionsInfo);
     }
   }
 
