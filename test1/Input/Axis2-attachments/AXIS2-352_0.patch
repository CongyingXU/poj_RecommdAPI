					<xs:documentation>
					MassPayRequestItemType
				</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="ReceiverEmail" type="ebl:EmailAddressType" minOccurs="0" maxOccurs="1">
						<annotation>
							<documentation>Email address of recipient. 
<br/>
								<br/>
								<b>Required</b>
