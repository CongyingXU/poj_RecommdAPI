typedef sequence<long> longSequence;
typedef sequence<string> stringSequence;

const long  upperBound   = 10;
typedef sequence<long, upperBound> boundedLongSequence;
typedef sequence<string, 2> boundedStringSequence;

typedef sequence<longSequence>            longSequenceSequence;
typedef sequence<boundedLongSequence>     boundedLongSequenceSequence;

typedef sequence<longSequence, 30>        longSequenceBoundedSequence;
typedef sequence<boundedLongSequence, 10> boundedLongSequenceBoundedSequence;


module sequences {
    typedef sequence< float > myFloat;
    typedef sequence< double > myDouble;
    typedef sequence< long double > myLongDouble;
    typedef sequence< short > myShort;
    typedef sequence< long > myLong;
    typedef sequence< long long > myLonglong;
    typedef sequence< unsigned short > myUnsignedShort;
    typedef sequence< unsigned long > myUnsignedLong;
    typedef sequence< unsigned long long > myUnsignedLongLong;
    typedef sequence< char > myChar;
    typedef sequence< wchar > myWchar;
    typedef sequence< boolean > myBoolean;
    typedef sequence< octet > myOctet;
    //typedef sequence< any > myAny;
};

