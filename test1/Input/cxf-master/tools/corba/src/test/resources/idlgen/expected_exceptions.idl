module TestException {
    struct BadRecord {
        string reason;
        short code;
    };
    exception BadRecordException {
        string reason;
        short code;
    };
    interface ExceptionTest {
        void
        review_data(
            in string data
        ) raises(
            ::TestException::BadRecordException,
            ::MyExceptionMessage
        );
    };
};
struct MyExceptionType {
    string message;
    short code;
    string company;
};
exception MyExceptionMessage {
    string message;
    short code;
    string company;
};
