+      names = ['long', 'short','both']
+      queues = java.util.HashSet.new
+      if queue_name.kind_of?(String)
+        queues.add(queue_name)
+        if !(names.include?(queue_name))
+          raise(ArgumentError, "Not support quene [ " + queue_name + " ]")
+        end
+      elsif queue_name.kind_of?(Array)
+        queue_name.each do |s|
+          queues.add(s)
+          if !(names.include?(s))
+            raise(ArgumentError, "Not support quene [ " + s + " ]")
+          end
+        end
+      else
+        raise(ArgumentError, "argument error #{queue_name}")
+      end
+      @admin.clearCompactQueues(ServerName.valueOf(server_name), queues)
+    end
