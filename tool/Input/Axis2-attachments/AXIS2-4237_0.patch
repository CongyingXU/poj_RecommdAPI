+
+
+        String[] array = null;
+        String value = null;
+        array = produces.value();
+        for (String s : array) {
+            if (value == null) {
+                value = s;
+            } else {
+                value = value + "," + s;
+            }
+        }
+
+        classModel.setProduces(value);
+
+    }
+
+    /**
